{
	"name": "archangel",
	"version": "3.0.0",
	"description": "Private bot for PopulousGaming",
	"author": "Jeroen Claassens <support@favware.tech>",
	"license": "Apache-2.0",
	"main": "dist/ArcAngel.js",
	"private": true,
	"type": "module",
	"imports": {
		"#lib/*": "./dist/lib/*.js",
		"#root/*": "./dist/*.js",
		"#utils/*": "./dist/lib/util/*.js",
		"#lib/env": "./dist/lib/env/index.js"
	},
	"scripts": {
		"start": "node --enable-source-maps dist/ArchAngel.js",
		"lint": "eslint src --ext ts --fix",
		"format": "prettier --write --loglevel=warn \"src/**/*.ts\"",
		"update": "yarn upgrade-interactive --latest",
		"build": "node scripts/build.mjs",
		"watch": "node scripts/watch.mjs",
		"clean": "node scripts/clean.mjs",
		"typecheck": "tsc -p src --noEmit",
		"dev": "yarn build && yarn start"
	},
	"dependencies": {
		"@discordjs/collection": "^0.3.2",
		"@sapphire/decorators": "^3.1.4",
		"@sapphire/discord-utilities": "^2.2.3",
		"@sapphire/discord.js-utilities": "^4.1.1",
		"@sapphire/fetch": "^2.0.3",
		"@sapphire/framework": "^2.1.4",
		"@sapphire/plugin-editable-commands": "^1.0.2",
		"@sapphire/plugin-logger": "^2.0.2",
		"@sapphire/time-utilities": "^1.4.7",
		"@sapphire/utilities": "^3.0.7",
		"@skyra/editable-commands": "^2.1.0",
		"@skyra/jaro-winkler": "^1.0.0",
		"bufferutil": "^4.0.5",
		"colorette": "^2.0.16",
		"discord-api-types": "^0.24.0",
		"discord.js": "^13.3.1",
		"dotenv-cra": "^3.0.2",
		"marked": "^3.0.8",
		"node-html-to-image": "^3.2.0",
		"reflect-metadata": "^0.1.13",
		"tslib": "^2.3.1",
		"utf-8-validate": "^5.0.7",
		"ws": "^8.2.3",
		"zlib-sync": "^0.1.7"
	},
	"devDependencies": {
		"@sapphire/eslint-config": "^4.0.3",
		"@sapphire/prettier-config": "^1.2.4",
		"@sapphire/ts-config": "^3.1.4",
		"@skyra/discord-components-core": "^2.11.2",
		"@types/marked": "^3.0.3",
		"@types/node": "^16.11.6",
		"@types/puppeteer": "^5.4.4",
		"@types/ws": "^8.2.0",
		"esbuild": "^0.13.12",
		"eslint": "^8.2.0",
		"prettier": "^2.4.1",
		"pretty-quick": "^3.1.1",
		"typescript": "^4.4.4"
	},
	"engines": {
		"node": ">=16.6.0",
		"npm": ">=7.24.2"
	},
	"prettier": "@sapphire/prettier-config",
	"packageManager": "yarn@3.1.0"
}
