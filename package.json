{
	"name": "arcangel",
	"version": "1.0.0",
	"description": "Private bot for PopulousGaming",
	"author": "Jeroen Claassens <support@favware.tech>",
	"license": "Apache-2.0",
	"main": "dist/ArcAngel.js",
	"private": true,
	"scripts": {
		"pm2:delete": "pm2 delete ArchAngel",
		"pm2:logs": "pm2 logs ArchAngel --lines=200",
		"pm2:restart": "pm2 restart ArchAngel",
		"pm2:start": "pm2 start pm2.ecosystem.yml",
		"pm2:dev": "yarn pm2:start --env development",
		"pm2:stop": "pm2 stop ArchAngel",
		"start": "node dist/ArchAngel.js",
		"lint": "eslint src --ext ts --fix",
		"style": "prettier --write src/**/*.{ts,json}",
		"update": "yarn upgrade-interactive --latest",
		"build": "tsc -b src",
		"watch": "tsc -b src -w",
		"clean": "tsc -b src --clean",
		"dev": "yarn build && yarn start"
	},
	"dependencies": {
		"@klasa/cache": "^0.0.3",
		"@klasa/console": "^0.0.3",
		"@klasa/core": "dirigeants/core#build",
		"@klasa/rest": "^0.5.4",
		"@klasa/snowflake": "^0.0.1",
		"@klasa/stopwatch": "^0.0.1",
		"@klasa/timestamp": "^0.0.1",
		"@klasa/type": "^0.0.1",
		"@sapphire/utilities": "^1.0.2",
		"@skyra/decorators": "^2.3.0",
		"klasa": "dirigeants/klasa#build",
		"marked": "^1.2.2",
		"module-alias": "^2.2.2",
		"node-html-to-image": "^3.1.0",
		"reflect-metadata": "^0.1.13",
		"ws": "^7.3.1",
		"zlib-sync": "^0.1.7"
	},
	"devDependencies": {
		"@sapphire/eslint-config": "^2.1.1",
		"@sapphire/ts-config": "^2.0.0",
		"@sapphire/prettier-config": "^1.0.6",
		"@klasa/dapi-types": "^0.3.0",
		"@skyra/discord-components-core": "^2.0.2",
		"@types/marked": "^1.2.1",
		"@types/module-alias": "^2.0.0",
		"@types/node": "^14.14.22",
		"@types/puppeteer": "^5.4.2",
		"@types/ws": "^7.4.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.18.0",
		"eslint-config-prettier": "^7.2.0",
		"eslint-plugin-prettier": "^3.3.1",
		"prettier": "^2.2.1",
		"typescript": "^4.1.3"
	},
	"_moduleAliases": {
		"@utils": "dist/lib/util",
		"@lib": "dist/lib",
		"@root": "dist"
	},
	"resolutions": {
		"acorn": "^8.0.5",
		"minimist": "^1.2.5",
		"kind-of": "^6.0.3"
	},
	"prettier": "@sapphire/prettier-config"
}
