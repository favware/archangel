{
	"name": "arcangel",
	"version": "1.0.0",
	"description": "Private bot for PopulousGaming",
	"author": "Jeroen Claassens <support@favware.tech>",
	"license": "Apache-2.0",
	"main": "dist/ArcAngel.js",
	"private": true,
	"type": "module",
	"imports": {
		"#lib/*": "./dist/lib/*.js",
		"#root/*": "./dist/*.js",
		"#utils/*": "./dist/lib/util/*.js"
	},
	"scripts": {
		"predocker:build": "run-s clean build",
		"docker:build": "docker-compose build",
		"docker:start": "docker-compose up -d",
		"docker:logs": "docker-compose logs -f",
		"start": "node dist/ArchAngel.js",
		"lint": "eslint src --ext ts --fix",
		"format": "prettier --write --loglevel=warn \"{src,tests}/**/*.ts\"",
		"update": "yarn upgrade-interactive --latest",
		"build": "tsc -b src",
		"watch": "tsc -b src -w",
		"clean": "rimraf dist",
		"dev": "yarn build && yarn start"
	},
	"dependencies": {
		"@discordjs/collection": "^0.1.6",
		"@sapphire/decorators": "^2.0.4",
		"@sapphire/discord.js-utilities": "^1.4.3",
		"@sapphire/framework": "^1.0.0-alpha.11",
		"@sapphire/plugin-logger": "1.0.8",
		"@sapphire/time-utilities": "^1.2.9",
		"@sapphire/utilities": "^1.4.5",
		"@skyra/editable-commands": "^1.0.0",
		"bufferutil": "^4.0.3",
		"colorette": "^1.2.1",
		"discord.js": "^12.5.1",
		"marked": "^2.0.0",
		"node-html-to-image": "^3.1.0",
		"reflect-metadata": "^0.1.13",
		"tslib": "2.1.0",
		"utf-8-validate": "^5.0.4",
		"ws": "^7.4.3",
		"zlib-sync": "^0.1.7"
	},
	"devDependencies": {
		"@sapphire/eslint-config": "^2.1.2",
		"@sapphire/prettier-config": "^1.0.7",
		"@sapphire/ts-config": "^2.0.1",
		"@skyra/discord-components-core": "^2.0.2",
		"@types/marked": "^1.2.2",
		"@types/node": "^14.14.28",
		"@types/node-fetch": "^2.5.8",
		"@types/puppeteer": "^5.4.3",
		"@types/ws": "^7.4.0",
		"@typescript-eslint/eslint-plugin": "^4.15.1",
		"@typescript-eslint/parser": "^4.15.1",
		"discord-api-types": "^0.12.1",
		"eslint": "^7.20.0",
		"eslint-config-prettier": "^7.2.0",
		"eslint-plugin-prettier": "^3.3.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.2.1",
		"pretty-quick": "^3.1.0",
		"rimraf": "^3.0.2",
		"typescript": "4.2.0-beta"
	},
	"resolutions": {
		"acorn": "^8.0.5",
		"minimist": "^1.2.5",
		"kind-of": "^6.0.3"
	},
	"engines": {
		"node": ">=15",
		"npm": ">=6"
	},
	"prettier": "@sapphire/prettier-config"
}
