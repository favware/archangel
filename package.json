{
	"name": "arcangel",
	"version": "1.0.0",
	"description": "Private bot for PopulousGaming",
	"author": "Jeroen Claassens <support@favware.tech>",
	"license": "Apache-2.0",
	"main": "dist/ArcAngel.js",
	"private": true,
	"scripts": {
		"pm2:delete": "pm2 delete ArchAngel",
		"pm2:logs": "pm2 logs ArchAngel --lines=200",
		"pm2:restart": "pm2 restart ArchAngel",
		"pm2:start": "pm2 start pm2.ecosystem.yml",
		"pm2:dev": "yarn pm2:start --env development",
		"pm2:stop": "pm2 stop ArchAngel",
		"start": "node dist/ArchAngel.js",
		"lint": "eslint src --ext ts --fix",
		"style": "prettier --write src/**/*.{ts,json}",
		"up": "yarn upgrade-interactive --latest",
		"build": "tsc -b src",
		"watch": "tsc -b src -w",
		"clean": "tsc -b src --clean",
		"dev": "yarn build && yarn start"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/favware/archangel.git"
	},
	"keywords": [
		"@klasa/core",
		"klasa",
		"bot",
		"standalone",
		"private",
		"populous_gaming"
	],
	"bugs": {
		"url": "https://github.com/favware/archangel/issues"
	},
	"dependencies": {
		"@klasa/cache": "^0.0.3",
		"@klasa/console": "^0.0.3",
		"@klasa/core": "dirigeants/core#build",
		"@klasa/rest": "^0.5.4",
		"@klasa/snowflake": "^0.0.1",
		"@klasa/stopwatch": "^0.0.1",
		"@klasa/timestamp": "^0.0.1",
		"@klasa/type": "^0.0.1",
		"@sapphire/utilities": "^1.0.2",
		"@skyra/decorators": "^2.3.0",
		"klasa": "dirigeants/klasa#build",
		"marked": "^1.2.2",
		"module-alias": "^2.2.2",
		"node-html-to-image": "favna/node-html-to-image#build",
		"reflect-metadata": "^0.1.13",
		"ws": "^7.3.1",
		"zlib-sync": "^0.1.7"
	},
	"devDependencies": {
		"@favware/eslint-config": "^6.0.1",
		"@favware/ts-config": "^3.0.4",
		"@klasa/dapi-types": "^0.3.0",
		"@skyra/discord-components-core": "^2.0.1",
		"@types/marked": "^1.1.0",
		"@types/module-alias": "^2.0.0",
		"@types/node": "^14.14.3",
		"@types/puppeteer": "^3.0.2",
		"@types/ws": "^7.2.7",
		"typescript": "^4.0.3"
	},
	"_moduleAliases": {
		"@utils": "dist/lib/util",
		"@lib": "dist/lib",
		"@root": "dist"
	},
	"resolutions": {
		"acorn": "^8.0.4",
		"minimist": "^1.2.5",
		"kind-of": "^6.0.3"
	}
}
